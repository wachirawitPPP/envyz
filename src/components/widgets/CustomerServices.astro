<section class="services py-8">
  <h2 class="text-2xl font-bold text-center mb-6">บริการของเรา</h2>
  <div class="grid grid-cols-1 md:grid-cols-5 gap-6 container mx-auto px-4">
    {
      customerservices.map((customer) => (
        <div class="card flex flex-col items-center bg-white shadow-md rounded-lg p-4 transition-transform transform hover:scale-105 hover:shadow-lg">
          <img
            src={customer.image}
            alt={customer.alt}
            class="h-20 w-20 object-cover rounded-full mb-4 transition-transform transform hover:scale-110"
          />
          <h3 class="text-lg font-semibold">{customer.title}</h3>
        </div>
      ))
    }
   
    <!-- Repeat similar structure for other cards -->
  </div>
</section>
